{
  "name": "gatsby-source-gravatar",
  "version": "1.0.1",
  "description": "Gatsby plugin to source Gravatar URLs from emails.",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src/**",
    "gatsby-node.js"
  ],
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "gravatar"
  ],
  "repository": "https://github.com/kaleabmelkie/gatsby-source-gravatar.git",
  "bugs": {
    "url": "https://github.com/kaleabmelkie/gatsby-source-gravatar/issues"
  },
  "homepage": "https://github.com/kaleabmelkie/gatsby-source-gravatar#readme",
  "author": "Kaleab S. Melkie <kaleabmelkie@gmail.com> (https://bit.ly/kaleab)",
  "license": "MIT",
  "private": false,
  "scripts": {
    "format": "prettier --write \"**/*.{tsx,ts,jsx,js,json,md,yml,graphql}\"",
    "test": "echo \"No tests yet.\" && exit 0"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "husky": "^6.0.0",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && git update-index --again"
    }
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": true
  }
}
